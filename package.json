{
  "name": "peony-webapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start": "vite --open",
    "local": "pnpm start --mode local",
    "dev": "pnpm start --mode dev",
    "stg": "pnpm start --mode stg",
    "prod": "pnpm start",
    "preview": "vite preview",
    "generate-pwa-assets": "npx pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "build-type-check": "run-p type-check \"build {@}\" --",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build": "vite build"
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "ant-design-vue": "^4.2.6",
    "apexcharts": "^4.1.0",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^4.5.1",
    "firebase": "^11.8.1",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.4.29",
    "vue-i18n": "^10.0.3",
    "vue-qr": "^4.0.9",
    "vue-qrcode-reader": "^5.7.0",
    "vue-router": "^4.3.3",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.23",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "naive-ui": "^2.39.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "sass-embedded": "^1.79.4",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vfonts": "^0.0.3",
    "vite": "^5.3.1",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.2.8",
    "workbox-window": "^7.1.0"
